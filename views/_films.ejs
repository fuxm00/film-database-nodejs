<table>
    <tr>
        <th>Title</th>
        <th>Year</th>
        <th>UserId</th>
        <th>FilmId</th>

    </tr>

    <% for (const film of films) { %>
        <tr>
            <td><%= film.title %></td>
            <td><%= film.year %></td>
            <td><%= film.userId %></td>
            <td><%= film.id %></td>

            <% if (!film.favourite) { %>
                <td><a href="/favourites/<%= film.id %>">ü§ç</a></td>
            <% } else { %>
                <td><a href="/remove-favourite/<%= film.id %>">üß°</a></td>
            <% } %>

            <% if (!film.toWatch) { %>
                <td><a href="/add-to-watch/<%= film.id %>">‚¨ú</a></td>
            <% } else { %>
                <td><a href="/remove-from-to-watch/<%= film.id %>">üëÄ</a></td>
            <% } %>

        </tr>
    <% } %>
</table>
